{
  "name": "errorreporter",
  "version": "1.0.99",
  "license": "CC-BY-3.0",
  "description": "Error reporter for client.",
  "homepage": "http://torben.website/errorReporter",
  "documentationWebsite": {
    "name": "errorReporter",
    "tagline": "Error reporter for client.",
    "trackingCode": "UA-40192634-22"
  },
  "keywords": [
    "client",
    "error",
    "reporter"
  ],
  "files": [
    "index.js"
  ],
  "main": "index.compiled.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/thaibault/errorReporter"
  },
  "scripts": {
    "build": "webOptimizer build",
    "build:export": "webOptimizer build '{__reference__: \"export\"}'",
    "build:export:compatible": "webOptimizer build '{__reference__: [\"export\", \"exportCompatible\"]}'",
    "clear": "webOptimizer clear",
    "document": "webOptimizer document",
    "lint": "webOptimizer check:type; webOptimizer lint",
    "prepare": "yarn build:export:compatible",
    "report:coverage": "nyc report --reporter=text-lcov | coveralls",
    "serve": "webOptimizer serve",
    "start": "yarn serve",
    "test": "nyc yarn test:plain",
    "test:browser": "webOptimizer test:browser",
    "test:plain": "webOptimizer test",
    "update": "yarn version --patch && yarn publish",
    "update:documentation": "documentationWebsite --log-level info",
    "watch": "webOptimizer build --watch"
  },
  "peerDependencies": {
    "ua-parser-js": "*"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "babel-plugin-transform-proto-to-assign": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "clientnode": "^3.0.86",
    "coveralls": "^3.0.2",
    "documentation-website": "^1.0.137",
    "eslint": "^5.4.0",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-jsdoc": "^3.7.1",
    "flow-bin": "^0.79.1",
    "jsdoc": "^3.5.2",
    "nyc": "^12.0.2",
    "qunit": "^2.6.2",
    "ua-parser-js": "^0.7.18",
    "weboptimizer": "^2.0.190",
    "webpack": "^4.17.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  },
  "webOptimizer": {
    "export": {
      "development": {
        "tool": false
      },
      "module": {
        "preprocessor": {
          "javaScript": {
            "options": {
              "plugins": {
                "__remove__": "transform-runtime"
              }
            }
          }
        }
      }
    },
    "exportCompatible": {
      "module": {
        "preprocessor": {
          "javaScript": {
            "options": {
              "plugins": {
                "__append__": "transform-proto-to-assign"
              },
              "presets": [
                "env",
                "stage-0"
              ]
            }
          }
        }
      }
    },
    "files": {
      "compose": {
        "cascadingStyleSheet": null
      }
    },
    "injection": {
      "external": {
        "implicit": {
          "pattern": {
            "exclude": {
              "__append__": {
                "__evaluate__": "/^clientnode\\/test(?:\\.compiled)?(?:\\.js)?$/"
              }
            }
          }
        }
      }
    },
    "module": {
      "aliases": {
        "#": "We want to use the pre-compiled artefacts.",
        "clientnode/test$": "clientnode/test.compiled"
      }
    }
  }
}
