{
  "name": "errorreporter",
  "version": "1.0.46",
  "license": "CC-BY-3.0",
  "description": "Error reporter for client.",
  "homepage": "http://torben.website/errorReporter",
  "documentationWebsite": {
    "name": "errorReporter",
    "tagline": "Error reporter for client.",
    "trackingCode": "UA-40192634-22"
  },
  "keywords": [
    "client",
    "error",
    "reporter"
  ],
  "files": [
    "index.js"
  ],
  "main": "index.compiled.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/thaibault/errorReporter"
  },
  "scripts": {
    "build": "webOptimizer build",
    "build:dll": "webOptimizer build:dll",
    "build:export": "webOptimizer build '{development:{tool:false},module:{preprocessor:{javaScript:{options:{plugins:{__remove__:\"transform-runtime\"}}}}}}'",
    "build:export:compatible": "webOptimizer build '{development:{tool:false},module:{preprocessor:{javaScript:{options:{plugins:{__remove__:\"transform-runtime\",__append__:\"transform-proto-to-assign\"},presets:[\"env\",\"stage-0\"]}}}}}'",
    "clear": "webOptimizer clear",
    "document": "webOptimizer document",
    "lint": "webOptimizer check:type; webOptimizer lint",
    "prepare": "npm run build:export:compatible",
    "report:coverage": "nyc report --reporter=text-lcov | coveralls",
    "serve": "webOptimizer serve",
    "start": "npm run serve",
    "test": "nyc npm run test:plain",
    "test:browser": "webOptimizer test:browser",
    "test:plain": "webOptimizer test",
    "update": "npm version patch && npm publish",
    "update:documentation": "documentationWebsite --log-level info",
    "watch": "webOptimizer build --watch",
    "watch:dll": "webOptimizer build:dll --watch"
  },
  "peerDependencies": {
    "ua-parser-js": "*"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "babel-plugin-transform-proto-to-assign": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "clientnode": "^1.0.326",
    "coveralls": "^3.0.0",
    "documentation-website": "^1.0.93",
    "eslint": "^4.18.0",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-flowtype": "^2.42.0",
    "eslint-plugin-jsdoc": "^3.4.1",
    "flow-bin": "^0.66.0",
    "jsdoc": "^3.5.2",
    "nyc": "^11.5.0",
    "qunit": "^2.5.0",
    "source-map-support": "^0.5.3",
    "ua-parser-js": "^0.7.17",
    "weboptimizer": "^1.0.477",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.10.1"
  },
  "webOptimizer": {
    "injection": {
      "external": {
        "implicit": {
          "pattern": {
            "exclude": {
              "__append__": {
                "__evaluate__": "/^clientnode\\/test(?:\\.compiled)?(?:\\.js)?$/"
              }
            }
          }
        }
      },
      "internal": {
        "vendor": {
          "__evaluate__": "self.debug && 2 < self.givenCommandLineArguments.length && ['build:dll', 'serve', 'test:browser', 'watch:dll'].includes(self.givenCommandLineArguments[2]) ? ['babel-polyfill', 'clientnode'].concat(self.givenCommandLineArguments[2] === 'serve' ? [] : ['script!qunit', 'weboptimizer/browserAPI']) : []"
        }
      }
    },
    "module": {
      "aliases": {
        "clientnode/test$": "clientnode/test.compiled"
      },
      "optimizer": {
        "babelMinify": {
          "module": {
            "mangle": false
          }
        }
      }
    }
  }
}
